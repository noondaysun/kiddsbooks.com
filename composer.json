{
	"autoload": {
		"classmap": [
            "src/database/seeds",
            "src/database/factories"
        ],
        "psr-4": {
            "App\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "description": "Site for kiddsbooks.com",
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "homepage": "https://github.com/noondaysun/kiddsbooks.com",
    "license": "Apache License",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "noondaysun/kiddsbooks.com",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('src/.env') || copy('src/.env.example', 'src/.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "type": "project"
}